{
  "manifest_version": 2,
  "name": "ニコニコカレンダー",
  "description": "ニコニコカレンダーの拡張機能",
  "version": "1.3",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon1.png"
  },
  "permissions": [
    "alarms",
    "notifications",
    "tabs",
    "storage"
  ]
}
